{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "eofline": false,
        "interface-over-type-literal": false,
        "no-console": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}